<tr>
    {% if guest.guestWithAccount %}
        <td>{{ guest.guestWithAccount.user.email }}</td>
    {% else %}
        <td>{{ guest.guestWithoutAccount.email }}</td>

    {% endif %}
    <td>{{ guest.invitedAt|date('d-m-Y') }}</td>
    <td class="js-update-meeting-guest-td">
         {{ include('admin/meeting/__guestUpdateTd.html.twig') }}
    </td>
    <td><a href="#"
          class="text-danger js-delete-meeting-guest"
          data-url="{{ path('admin.meeting_guest.delete', {id_meeting: meeting.id, id: guest.id}) }}"
    >
        <span class="fa fa-times"></span></a></td>
</tr>