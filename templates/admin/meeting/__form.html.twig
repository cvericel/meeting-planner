{{ form_start(form) }}
    <div class="row">
        <div class="col-md-5">
            <div class="container">
                {{ form_row(form.title) }}
            </div>
        </div>
        <div class="col-md-5">
            {{ form_row(form.description) }}
        </div>
    </div>
    {{ form_widget(form) }}
    <button class="btn btn-primary"> {{ button|default('Enregistrer') }} </button>
{{ form_end(form) }}